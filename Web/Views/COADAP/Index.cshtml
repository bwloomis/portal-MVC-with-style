@{
    Layout = null;
}
<head>
    <title>Registration</title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
<link href="~/Content/css/jquery.datepick.css" rel="stylesheet" />
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/additional-methods.min.js"></script>
<script src="/Content/js/jquery.plugin.js"></script>
<script src="~/Content/js/jquery.datepick.js"></script>
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<script src="~/Scripts/bootstrap.min.js"></script>
    <style>
        .nav-tabs {
            background-color: white;
        }
        
        .nav-tabs > li {
            background-color: white;
        }

        .error {
            color: red;
            font-size: 0.8em;
        }

        .box {
            padding: 5px;
            background-color:#eeeeee;
            border:1px solid lightgray;
            margin: 10px;
        }

        .btn-file {
            position: relative;
            overflow: hidden;
        }
        .btn-file input[type=file] {
            position: absolute;
            top: 0;
            right: 0;
            min-width: 100%;
            min-height: 100%;
            font-size: 100px;
            text-align: right;
            filter: alpha(opacity=0);
            opacity: 0;
            outline: none;
            background: white;
            cursor: inherit;
            display: block;
        }

        input[type=radio] {
           display:none;
        }
        input[type=radio] + label {
            display: block;
            background-color: lightgray;
            padding: 8px;
            border: 3px solid white;
            text-align:center;
            font-weight: normal;
            border: 1px solid black;
            
        }
        input[type=radio]:checked + label {
            display: block;
            background-color: lightgreen;
            border: 3px solid black;
            font-weight:500;
        }
        #otherLang {
            margin-top:5px;
        }
    </style>

<script>
    $(document).ready(function () {
        var workers = 1;
        var incomes = 1;
        $("#birthDate").datepick();
        $("#unemployedDate").datepick();

        $("#otherLangRadio").click(function () {
            $('#otherLang').prop('disabled', false);
        });

        $(".langNotOther").click(function () {
            $("#otherLang").prop('disabled', true);
        });

        $("#otherRelRadio").click(function () {
            $('#otherRel').prop('disabled', false);
        });

        $(".relNotOther").click(function () {
            $("#otherRel").prop('disabled', true);
        });

        $(".notOtherDiff").click(function () {
            $("#otherDiff").prop('disabled', true);
            $("#cityOtherDiff").prop('disabled', true);
        });

        $("#whereDiffOther").click(function () {
            $("#otherDiff").prop('disabled', false);
            $("#cityOtherDiff").prop('disabled', false);
        });

        $(".notWhereDifferent").click(function () {
            $(".whereDiff").prop('disabled', true);
            $(".whereDiff").prop('checked', false);
        });

        $("#whereDifferent").click(function () {
            $(".whereDiff").prop('disabled', false);

        });
        $("#docYes").click(function () {
            $("#doctorName").prop('disabled', false);

        });
        $("#docNo").click(function () {
            $("#doctorName").prop('disabled', true);

        });

        $("#addWorker").click(function () {
            workers = workers + 1;
            $("#workers").append('<br /><div class="box col-sm-10"><div class="col-sm-8"><label>Name of worker (you, spouse, dependent, etc.) <input type="text" name="worker' + workers + 'Name"></label></div><div class="col-sm-8"><label>Employer Name <input type="text" name="worker' + workers + 'EmployerName"></label></div><div class="col-sm-8"><label>Start Date (or continuing) <input type="text" name="worker' + workers + 'StartDate"></label></div><div class="col-sm-8"><label>Is this work temporary or seasonal? <label> <input type="radio" name="worker' + workers + 'TempSeason" value="yes" /> Yes</label> <label> <input type="radio" name="worker' + workers + 'TempSeason" value="no" /> No</label> </label></div><div class="col-sm-8"><label>Monthly Amount (average) <input type="text" name="worker' + workers + 'Amount" id="worker' + workers + 'Amount"></label></div></div>');
        });


        $("#addIncome").click(function () {
            incomes = incomes + 1;
            $("#otherIncomes").append('<br /><div class="box col-sm-10"><div class="col-sm-8"><label>This check comes to: (me, my spouse, my child, etc.) </label> <input type="text" name="otherIncome' + incomes + 'To" /></div><div class="col-sm-8"><label>Type of Benefit or Income from list above (for example, "SSI") </label> <input type="text" name="otherIncome' + incomes + 'Type" /></div><div class="col-sm-8"><label>Monthly Amount (Gross Amount) </label> <input type="text" name="otherIncome' + incomes + 'Gross" /></div></div>');
        });

        jQuery.validator.setDefaults({
            debug: true,
            success: "valid"
        });

        $('.btn-file :file').on('fileselect', function (event, numFiles, label) {

            var input = $(this).parents('.input-group').find(':text'),
                log = numFiles > 1 ? numFiles + ' files selected' : label;

            if (input.length) {
                input.val(log);
            } else {
                if (log) alert(log);
            }

        });
        

        $("#inputForm").validate({
            rules: {
                zip: {
                    number: true
                },
                zipMail: {
                    number: true
                },
                childrenWith: {
                    number:true
                },
                children50: {
                    number:true
                },
                hoursPerWeek: {
                    number: true
                },
                premium: {
                    number: true
                },
                medApplyMonth: {
                    number: true,
                    minlength: 2,
                    maxlength: 2,
                    range: [01, 12]
                },
                medApplyYear: {
                    number:true,
                    minlength: 4,
                    maxlength: 4
                },
                monthPartA: {
                    number: true,
                    minlength: 2,
                    maxlength: 2,
                    range: [01, 12]
                },
                monthPartB: {
                    number: true,
                    minlength: 2,
                    maxlength: 2,
                    range: [01, 12]
                },
                monthPartD: {
                    number: true,
                    minlength: 2,
                    maxlength: 2,
                    range: [01, 12]
                },
                startMonth: {
                    number: true,
                    minlength: 2,
                    maxlength: 2,
                    range: [01, 12]
                },
                healthMonth: {
                    number: true,
                    minlength: 2,
                    maxlength: 2,
                    range: [01, 12]
                },
                diagMonth: {
                    number: true,
                    minlength: 2,
                    maxlength: 2,
                    range: [01, 12]
                },
                prevMonth: {
                    number: true,
                    minlength: 2,
                    maxlength: 2,
                    range: [01, 12]
                },
                dueDeliver: {
                    number: true,
                    minlength: 2,
                    maxlength: 2,
                    range: [01, 12]
                },
                yearPartA: {
                    number: true,
                    minlength: 4,
                    maxlength: 4
                },
                yearPartB: {
                    number: true,
                    minlength: 4,
                    maxlength: 4
                },
                yearPartD: {
                    number: true,
                    minlength: 4,
                    maxlength: 4
                },
                startYear: {
                    number: true,
                    minlength: 4,
                    maxlength: 4
                },
                healthYear: {
                    number: true,
                    minlength: 4,
                    maxlength: 4
                },
                diagYear: {
                    number: true,
                    minlength: 4,
                    maxlength: 4
                },
                prevYear: {
                    number: true,
                    minlength: 4,
                    maxlength: 4
                },
                phone1: {
                    phoneUS: true
                },
                phone2: {
                    phoneUS: true
                },
                phoneReturn: {
                    phoneUS: true
                }

            },
            highlight: function (element, errorClass) {
                $(element).closest('.form-group').addClass('has-error');
            },
            unhighlight: function (element, errorClass) {
                $(element).closest('.form-group').removeClass('has-error');
            }
        });
    });

    $(document).on('change', '.btn-file :file', function () {
        var input = $(this),
            numFiles = input.get(0).files ? input.get(0).files.length : 1,
            label = input.val().replace(/\\/g, '/').replace(/.*\//, '');
        input.trigger('fileselect', [numFiles, label]);
    });


</script>
</head>
<body>
    <div class="container">
        <div id="content">
            <div id="navigation" class="navbar-fixed-top">
                <div class="container">
                    <ul id="tabs" class="nav nav-tabs" data-tabs="tabs">
                        <li class="active"><a href="#personal" data-toggle="tab">Personal</a></li>
                        <li><a href="#medical" data-toggle="tab">Medical</a></li>
                        <li><a href="#financial" data-toggle="tab">Financial</a></li>
                    </ul>
                </div>
            </div>

            <div class="container" style="padding-top:60px;">
                    <div id="my-tab-content" class="tab-content">

                        <div class="tab-pane active" id="personal">
                            <form id="form-id" action="/Results/Save" method="post">
                                <input id="test" class="form-control" name="lastesttName" type="text" />
                                <input type="submit" value="Submit" id="btnSubmit" />
                            </form>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label col-sm-4" for="lastName">Full Legal Name (Last)</label>
                                        <div class="col-sm-7">
                                            <input id="lastName" class="form-control" name="lastName" type="text" />
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-sm-4" for="firstName">First</label>
                                        <div class="col-sm-7">
                                            <input id="firstName" class="form-control col-sm-3" name="firstName" type="text" />
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-sm-4" for="middleInitial">Middle Initial</label>
                                        <div class="col-sm-2">
                                            <input id="middleInitial" class="form-control col-sm-1" name="middleInitial" type="text" />
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-sm-4" for="birthDate">Date of birth</label>
                                        <div class="col-sm-5">
                                            <input id="birthDate" class="form-control col-sm-1" name="birthDate" type="text" />
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div>
                                        <label class="control-label col-sm-4">  What is your ethnicity? </label>
                                        <div class="col-sm-2">
                                            <input id="hispanic" type="radio" name="EthnicityID" value="hispanic" />
                                            <label for="hispanic">Hispalnic/Latino(a)</label>

                                        </div>
                                        <div class="col-sm-2">
                                            <input id="nonhispanic" type="radio" name="EthnicityID" value="nonhispanic" />
                                            <label for="nonhispanic">Non-Hispanic</label>
                                        </div>
                                        <div class="col-sm-2">
                                            <input id="ethunknown" type="radio" name="EthnicityID" value="unknown" />
                                            <label for="ethunknown">Unknown</label>

                                        </div>
                                        <div class="col-sm-2">
                                            <input id="ethnoanswer" type="radio" name="EthnicityID" value="noAnswer" />
                                            <label for="ethnoanswer">Prefer not to answer </label>

                                        </div>

                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <br />
                                    <div class="control-label col-sm-4">What is your race?</div>
                                    <div><strong>Check all that apply</strong></div>

                                    <div class="col-sm-offset-4">
                                        <div class="col-sm-3">
                                            <div class="checkbox">
                                                <label>
                                                    <input type="checkbox" name="RaceID" value="white" />
                                                    White
                                                </label>
                                            </div>
                                            <div class="checkbox">
                                                <label>
                                                    <input type="checkbox" name="RaceID" value="native" />
                                                    Native American/Pacific Islander
                                                </label>
                                            </div>
                                            <div class="checkbox">
                                                <label>
                                                    <input type="checkbox" name="RaceID" value="asian" />
                                                    Asian
                                                </label>
                                            </div>
                                            <div class="checkbox">
                                                <label>
                                                    <input type="checkbox" name="RaceID" value="noAnswer" />
                                                    Prefer not to answer
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-sm-3">
                                            <div class="checkbox">
                                                <label>
                                                    <input type="checkbox" name="RaceID" value="black" />
                                                    Black
                                                </label>
                                            </div>
                                            <div class="checkbox">
                                                <label>
                                                    <input type="checkbox" name="RaceID" value="indian" />
                                                    American Indian or Alaska Native
                                                </label>
                                            </div>
                                            <div class="checkbox">
                                                <label>
                                                    <input type="checkbox" name="RaceID" value="unknown" />
                                                    Unknown
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <br />
                                    <label class="control-label col-sm-4">What is your preferered language?</label>
                                    <div class="radio col-sm-2">

                                        <input id="langEng" type="radio" class="langNotOther" name="language" value="english" />
                                        <label for="langEng">English</label>

                                    </div>
                                    <div class="radio col-sm-2">

                                        <input id="langSp" type="radio" class="langNotOther" name="language" value="spanish" />
                                        <label for="langSp">Spanish</label>

                                    </div>
                                    <div class="radio col-sm-2">

                                        <input id="langFr" type="radio" class="langNotOther" name="language" value="french" />
                                        <label for="langFr">French</label>

                                    </div>
                                    <div class="radio col-sm-2">

                                        <input type="radio" id="otherLangRadio" name="language" value="other" />
                                        <label for="otherLangRadio">Other</label>

                                        <input type="text" name="otherLang" id="otherLang" disabled="disabled" />
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <br />
                                    <label class="control-label col-sm-4">What is your gender?</label>
                                    <div class="radio col-sm-2">

                                        <input id="genMale" type="radio" name="gender" value="male" />
                                        <label for="genMale">Male</label>

                                    </div>
                                    <div class="radio col-sm-2">

                                        <input id="genFemale" type="radio" name="gender" value="female" />
                                        <label for="genFemale">Female</label>

                                    </div>
                                    <div class="radio col-sm-2">

                                        <input id="transMtoF" type="radio" name="gender" value="transMtoF" />
                                        <label for="transMtoF">
                                            Transgender, male to female
                                        </label>
                                    </div>
                                    <div class="radio col-sm-2">

                                        <input id="transFtoM" type="radio" name="gender" value="transFtoM" />
                                        <label for="transFtoM">
                                            Transgender, female to male
                                        </label>
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <br />
                                    <label class="control-label col-sm-4">What is your housing status:</label>
                                    <div class="col-sm-8">
                                        <div class="radio">

                                            <input id="housPerm" type="radio" name="housing" value="perm" />
                                            <label for="housPerm">
                                                Permanent: I rent, own, or share my current residence
                                            </label>
                                        </div>
                                        <div class="radio">

                                            <input id="housTemp" type="radio" name="housing" value="temp" />
                                            <label for="housTemp">
                                                Temporary: I am in a temporary housing situation (homeless, halfway house, shelter, staying with friends)
                                            </label>
                                        </div>
                                        <div class="radio">

                                            <input id="housInst" type="radio" name="housing" value="inst" />
                                            <label for="housInst">
                                                I am in an institution (hospice, nursing home, etc.)
                                            </label>
                                        </div>
                                        <div class="radio">

                                            <input id="housUnknown" type="radio" name="housing" value="unknown" />
                                            <label for="housUnknown">
                                                Unknown or refuse to answer
                                            </label>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-12 box">

                                    <label class="control-label col-sm-4">What is your current <span style="font-weight:bold;">residential</span> address?</label>
                                    <div class="form-group col-sm-12">
                                        <label class="control-label col-sm-4" for="streetAddress">Street Address (PO Boxes will <span style="font-weight:bold;">NOT</span> be accepted)</label>
                                        <div class="col-sm-7">
                                            <input id="streetAddress" class="form-control" name="streetAddress" type="text" />
                                        </div>
                                    </div>
                                    <div>
                                        <div class="form-group col-sm-5">
                                            <label class="control-label col-sm-4" for="city">City</label>
                                            <div class="col-sm-7">
                                                <input id="city" class="form-control" name="city" type="text" />
                                            </div>
                                        </div>
                                        <div class="form-group col-sm-3">
                                            <label class="control-label col-sm-4" for="state">State</label>
                                            <div class="col-sm-7">
                                                <input id="state" class="form-control" name="state" type="text" />
                                            </div>
                                        </div>
                                        <div class="form-group col-sm-4">
                                            <label class="control-label col-sm-4" for="zip">ZIP Code</label>
                                            <div class="col-sm-7">
                                                <input id="zip" class="form-control" name="zip" type="text" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-sm-7">
                                        <label class="control-label col-sm-offset-4 col-sm-3" for="county">County</label>
                                        <div class="col-sm-4">
                                            <input id="county" class="form-control" name="county" type="text" />
                                        </div>
                                    </div>

                                    <div>
                                        <label class="control-label col-sm-6">May we contact you at this address?</label>
                                        <div class="radio col-sm-3">

                                            <input id="contactResYes" type="radio" name="contact" value="yes" />
                                            <label for="contactResYes">
                                                Yes
                                            </label>
                                        </div>
                                        <div class="radio col-sm-3">

                                            <input id="contactResNo" type="radio" name="contact" value="no" />
                                            <label for="contactResNo">
                                                No
                                            </label>
                                        </div>
                                    </div>
                                    <div class="col-sm-7 col-sm-offset-3">

                                        <div class="form-group">
                                            <label for="proofOfAddress" class="control-label">
                                                You must attach proof that you live at this address. Please see the instructions for the kind of proof ADAP
                                                will accept.
                                            </label>
                                            <div class="input-group">
                                                <span class="input-group-btn">
                                                    <span class="btn btn-primary btn-file">
                                                        Browse
                                                        <input type="file" id="proofOfAddress" class="form-control" name="proofOfAddress" />
                                                    </span>
                                                </span>
                                                <input type="text" class="form-control" readonly />
                                            </div>

                                        </div>
                                    </div>
                                </div>

                                <div class="col-sm-12 box">

                                    <label class="control-label col-sm-4">What is your current <span style="font-weight:bold;">mailing</span> address (if different than your residential)?</label>
                                    <div class="form-group col-sm-12">
                                        <label class="control-label col-sm-4" for="streetAddressMail">Street Address (PO Boxes <span style="font-weight:bold;">will</span> be accepted, but not outside Colorado)</label>
                                        <div class="col-sm-7">
                                            <input id="streetAddressMail" class="form-control" name="streetAddressMail" type="text" />
                                        </div>
                                    </div>
                                    <div>
                                        <div class="form-group col-sm-5">
                                            <label class="control-label col-sm-4" for="cityMail">City</label>
                                            <div class="col-sm-7">
                                                <input id="cityMail" class="form-control" name="cityMail" type="text" />
                                            </div>
                                        </div>
                                        <div class="form-group col-sm-3">
                                            <label class="control-label col-sm-4" for="stateMail">State</label>
                                            <div class="col-sm-7">
                                                <input id="stateMail" class="form-control" name="stateMail" type="text" />
                                            </div>
                                        </div>
                                        <div class="form-group col-sm-4">
                                            <label class="control-label col-sm-4" for="zipMail">ZIP Code</label>
                                            <div class="col-sm-7">
                                                <input id="zipMail" class="form-control" name="zipMail" type="text" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-sm-7">
                                        <label class="control-label col-sm-offset-4 col-sm-3" for="countyMail">County</label>
                                        <div class="col-sm-4">
                                            <input id="countyMail" class="form-control" name="countyMail" type="text" />
                                        </div>
                                    </div>

                                    <div>
                                        <label class="control-label col-sm-6">May we contact you at this address?</label>
                                        <div class="radio col-sm-3">

                                            <input id="contactMailYes" type="radio" name="contactMail" value="yes" />
                                            <label for="contactMailYes">
                                                Yes
                                            </label>
                                        </div>
                                        <div class="radio col-sm-3">

                                            <input id="contactMailNo" type="radio" name="contactMail" value="no" />
                                            <label for="contactMailNo">
                                                No
                                            </label>
                                        </div>
                                    </div>
                                </div>
                                <div>
                                    <label class="control-label col-sm-4">At what phone numbers can we reach you during daytime hours?</label>
                                    <div class="col-sm-12 box">
                                        <div class="form-group col-sm-7">
                                            <label for="phone1" class="control-label col-sm-offset-4 col-sm-3">Phone Number</label>
                                            <div class="col-sm-4">
                                                <input id="phone1" class="form-control" type="tel" name="phone1" />
                                            </div>
                                        </div>
                                        <div class="checkbox col-sm-1">
                                            <label>
                                                <input type="checkbox" name="phone1type" value="home" />
                                                Home
                                            </label>
                                        </div>
                                        <div class="checkbox col-sm-1">
                                            <label>
                                                <input type="checkbox" name="phone1type" value="work" />
                                                Work
                                            </label>
                                        </div>
                                        <div class="checkbox col-sm-1">
                                            <label>
                                                <input type="checkbox" name="phone1type" value="cell" />
                                                Cell phone
                                            </label>
                                        </div>
                                        <br />
                                        <div class="col-md-12">
                                            <label class="control-label col-sm-4">May we leave a message on this phone?</label>
                                            <div class="radio col-sm-3">

                                                <input id="phone1msgYes" type="radio" name="phone1message" value="yes" />
                                                <label for="phone1msgYes">
                                                    Yes
                                                </label>
                                            </div>
                                            <div class="radio col-sm-3">

                                                <input id="phone1msgNo" type="radio" name="phone1message" value="no" />
                                                <label for="phone1msgNo">
                                                    No
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-12 box">
                                        <div class="form-group col-sm-7">
                                            <label for="phone2" class="control-label col-sm-offset-4 col-sm-3">Phone Number</label>
                                            <div class="col-sm-4">
                                                <input id="phone2" class="form-control" type="tel" name="phone1" />
                                            </div>
                                        </div>
                                        <div class="checkbox col-sm-1">
                                            <label>
                                                <input type="checkbox" name="phone2type" value="home" />
                                                Home
                                            </label>
                                        </div>
                                        <div class="checkbox col-sm-1">
                                            <label>
                                                <input type="checkbox" name="phone2type" value="work" />
                                                Work
                                            </label>
                                        </div>
                                        <div class="checkbox col-sm-1">
                                            <label>
                                                <input type="checkbox" name="phone2type" value="cell" />
                                                Cell phone
                                            </label>
                                        </div>
                                        <div class="col-md-12">
