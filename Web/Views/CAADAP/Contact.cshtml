            $(document).ready(function () {
                $("#C1_MemberDateOfBirth").off('change').on('change', function () {
                    var dob = $('#C1_MemberDateOfBirth').val();
                    var dobSplit = dob.split("/");
                    var DobYear = parseInt(dobSplit[2]);
                    var currentYear = (new Date).getFullYear();
                    debugger;
                    if (DobYear > (currentYear - 18)) {
                        alert('You must be 18 years or older. Enter your correct date of birth.');
                        $("#C1_MemberDateOfBirth").val('');

                    } else if ((DobYear < (currentYear - (120)))) {
                        alert('You must be 120 years or younger. Enter your correct date of birth.');
                        $("#C1_MemberDateOfBirth").val('');

                    }


                });


            });
        </script>
        <div class="form-group">

