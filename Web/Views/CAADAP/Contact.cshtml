        <div class="form-group" style="margin-left:12px">
            @Helpers.LabeledCheckboxInput(Model, "C1_AddressHomeless")
        </div>
        <script>
            $(document).ready(function () {
                HideMaillindAddandResadd();
                $("#C1_AddressHomeless").change(HideMaillindAddandResadd);
            });
            function HideMaillindAddandResadd() {
                if ($("#C1_AddressHomeless").get(0).checked) {

                    $(".divresadd").show();
                    $(".divmailadd").show();
                    $(".divprovider").show();
                }
                else {

                    $(".divresadd").show();
                    $(".divmailadd").show();
                    $(".divprovider").hide();

                }
            };

        </script>
        <script type="text/javascript">
            $(function () {
                $(".divotherproofatch").hide();

                $("#C1_AddressProofOfResidencyType").change(function () {
                    var selectedText = $(this).find("option:selected").text();
                    var selectedValue = $(this).val();
                    // alert("Selected Text: " + selectedText + " Value: " + selectedValue);
                    if (selectedText == 'Other') {
                        $(".divotherproofatch").show();
                    }
                    else {
                        $(".divotherproofatch").hide();

                    }

                }).change();
            });
        </script>

        <div class="divprovider">
