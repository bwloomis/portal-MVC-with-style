<html>
<head>
    <title>SIS Online</title>

    <!--<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script> -->
    <script src="/Scripts/jquery-2.1.3.min.js"></script>
    <script src="/Scripts/jquery.highlight.js" type="text/javascript"></script>
    <link href="/Content/js/jquery-ui-1.11.2-custom/jquery-ui.min.css" rel="stylesheet" />
    <!--<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.3/jquery-ui.min.js"></script> -->
    <script src="/Scripts/jquery-ui-1.11.4.min.js"></script>
    <script src="/Scripts/moment.js" type="text/javascript"></script>
    <script src="/Scripts/spin.min.js" type="text/javascript"></script>
    <script src="/Scripts/gridmvc.min.js" type="text/javascript"></script>
    <script src="/Content/js/DateFunctionForSearch.js" type="text/javascript"></script>

    <script src="/Scripts/bootstrap.min.js"></script>
    <link href="/Content/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/themes/base/jquery.ui.datepicker.css" rel="stylesheet" type="text/css" />

    <script>
    $.ajax({
        type: "GET",
        url: "/Account/IsLoggedIn",
        async: false,
        cache: false,
        success: function (result) {

            if (result == "False") {
                var url = window.location.href;
                var arr = url.split("/");
                var location = arr[0] + "//" + arr[2];
                window.location = location;
            }
        },
        error: function (xhr, status, err) {
            alert("Error checking login status.  Status: " + xhr.status + "  Message: " + err);
        }
    });
</script>
    <style>
        .topBar {
            font-weight: bold;
        }

        .sisImage {
            position: absolute;
            top: 45px;
            left: -45px;
            z-index: -3;
            height: 180px;
            width: auto;
        }

        

        nav {
            background: none repeat scroll 0 0 #8E3C42;
            display: block;
            width: 100%;
            //1580px;
            font-size: 1.6em;
            min-height: 32px;
            line-height: 32px;
            position: absolute;
            top: 0px;
            left: 0px;
            z-index: 9999;
            color: white;
            vertical-align: middle;
            padding-left: 10px;
        }

            nav > div > a {
                float: left;
            }

        .searchTitle {
            float: left;
        }

        .topNavItems {
            float: right;
        }

            .topNavItems > div {
                position: relative;
                line-height: 80%;
                margin-top: 6px;
                float: left;
                margin-left: 5px;
                font-size: small;
                padding: 2px;
            }

       
        a {
            color: #193773;
            font-family: verdana,Helvetica,sans-serif;
            font-size: 10px;
            font-weight: bold;
            text-decoration: none;
        }


        .divInline {
            display: inline-block;
            text-align: left;
            vertical-align: top;
            margin: 0px 10px;
        }

        #main {
            position: absolute;
            top: 170px;
            left: 150px;
            font-size: large;
            padding-bottom: 10px;
        }

        #startProcess {
            font-size: large;
        }
    </style>

    <link rel="stylesheet" type="text/css" href="/Content/css/sitewide.css">
    <link rel="alternate stylesheet" type="text/css" media="screen" title="medium" href="/Content/css/sitewide2.css">
    <link rel="alternate stylesheet" type="text/css" media="screen" title="large" href="/Content/css/sitewide3.css">
    <script language="JavaScript" src="/content/js/textchanger.js" type="text/javascript"></script>
</head>
<body>

    <header>
        <nav>

            @{string navTitle = "Error"; }

            <div style="font-weight:bold; color:white;font-size: 1.5em; ">

                <div class="searchTitle">
                    @Html.Raw(navTitle)
                </div>

                   
            </div>

        </nav>
    </header>
    <div>
        <a href="~/Search">
            <img class="sisImage" src="~/Content/images/SISOnline logo.jpg" />
        </a>

    </div>

    <div id="main" class="col-sm-10">
        @RenderBody()
    </div>

</body>
</html>
