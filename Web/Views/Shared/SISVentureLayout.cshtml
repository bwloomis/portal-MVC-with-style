
<html>
<head>
    <title>SIS Venture</title>

    <!--<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script> -->
    <script src="/Scripts/jquery-2.1.3.min.js"></script>
    <script src="/Scripts/jquery.highlight.js" type="text/javascript"></script>
    <link href="/Content/js/jquery-ui-1.11.2-custom/jquery-ui.min.css" rel="stylesheet" />
    <!--<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.3/jquery-ui.min.js"></script> -->
    <script src="/Scripts/jquery-ui-1.11.4.min.js"></script>
    <script src="/Scripts/moment.js" type="text/javascript"></script>
    <script src="/Scripts/spin.min.js" type="text/javascript"></script>
    <script src="/Scripts/gridmvc.min.js" type="text/javascript"></script>
    <script src="/Content/js/DateFunctionForSearch.js" type="text/javascript"></script>

    <script src="/Scripts/bootstrap.min.js"></script>
    <link href="/Content/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/themes/base/jquery.ui.datepicker.css" rel="stylesheet" type="text/css" />

    <script>
    $.ajax({
        type: "GET",
        url: "/Account/IsLoggedIn",
        async: false,
        cache: false,
        success: function (result) {

            if (result == "False") {
                var url = window.location.href;
                var arr = url.split("/");
                var location = arr[0] + "//" + arr[2];
                window.location = location;
            }
        },
        error: function (xhr, status, err) {
            alert("Error checking login status.  Status: " + xhr.status + "  Message: " + err);
        }
    });
    </script>
    
    <script src="~/Content/js/jquery.sessionTimeout.js"></script>
    <script>
        var timeout = @Assmnts.Infrastructure.SessionHelper.SessionTotalTimeoutMinutes;
    </script>
    <script src="~/Content/js/timeout.js"></script>
    
    <style>
        .topBar {
            font-weight: bold;
        }
        .sisImage {
            position: absolute;
            top: 45px;
            left: -45px;
            z-index: -3;
            height: 180px;
            width: auto;
        }

        .topBoxes {
            position: absolute;
            top: 45px;
            left: 300px;
            color: #8E3C42;
            border-color: #4D4B53;
            height: 112px;
            width: 800px;
        }

            .topBoxes a {
                color: #8E3C42;
                font-weight: normal;
                text-decoration: underline;
                margin-left: 3px;
            }

            .topBoxes > span, .head {
                font-weight: bold;
                font-size: inherit;
            }
        .adminBox {
            position:absolute;
            padding-top: 2px;
            padding-right: 40px;
            padding-left: 5px; 
            width: 400px;
            height: 100%; 
            border: solid;
            border-width: 2px;
            border-color: inherit;
            text-align:left;
        }

        nav {
            background: none repeat scroll 0 0 #8E3C42;
            display: block;
            width: 100%;
            //1580px;
            font-size: 1.6em;
            min-height: 32px;
            line-height: 32px;
            position: absolute;
            top: 0px;
            left: 0px;
            z-index: 9999;
            color: white;
            vertical-align: middle;
            padding-left: 10px;
        }

            nav > div > a {
                float: left;
            }

        .searchTitle {
            float: left;
        }

        .topNavItems {
            float: right;
        }

            .topNavItems > div {
                position: relative;
                line-height: 80%;
                margin-top: 6px;
                float: left;
                margin-left: 5px;
                font-size: small;
                padding: 2px;
            }

        .topButton {
            height: inherit;
            vertical-align: middle;
            /*font-size:small;*/
            color: #4D4B53;
        }

        .userBox {
            position: relative;
            line-height: 70%;
            margin-top: 6px;
            float: left;
            margin-left: 50px;
            background-color: white;
            color: #4D4B53;
            border-style: solid;
            border-width: 1px;
            border-color: #4A6096;
            font-size: small;
            padding: 6px;
        }

        .textSizeChangers {
            position: relative;
            line-height: 70%;
            margin-top: 6px;
            float: left;
        }

        .minutesBox {
            position: relative;
            line-height: 70%;
            margin-top: 6px;
            float: left;
            margin-left: 50px;
            background-color: white;
            color: #4D4B53;
            border-style: solid;
            border-width: 1px;
            border-color: #4A6096;
            font-size: small;
            padding: 6px;
            padding-top: 2px;
            padding-bottom: 2px;
        }

        a {
            color: #193773;
            font-family: verdana,Helvetica,sans-serif;
            font-size: 10px;
            font-weight: bold;
            text-decoration: none;
        }

     
        .divInline {
            display: inline-block;
            text-align: left;
            vertical-align: top;
            margin: 0px 10px;
        }
        #main {
            position: absolute;
            top: 170px;
            left: 150px;
            font-size:large;
            background-color: #e1e9f8;
            border: 2px groove;
            padding-bottom: 10px;
        }
        #startProcess {
            font-size: large;
        }
    </style>

    <link rel="stylesheet" type="text/css" href="/Content/css/sitewide.css">
    <link rel="alternate stylesheet" type="text/css" media="screen" title="medium" href="/Content/css/sitewide2.css">
    <link rel="alternate stylesheet" type="text/css" media="screen" title="large" href="/Content/css/sitewide3.css">
    <script language="JavaScript" src="/content/js/textchanger.js" type="text/javascript"></script>
    <script>
        function returnResults() {
            window.location = '/Search';
        }
    </script>
</head>
<body>
   
    <header>
        <nav>

            @{string navTitle = "Venture Data Sync"; }

            <div class="topBar">

                <div class="searchTitle">
                    @Html.Raw(navTitle)
                </div>

                <div class="topNavItems">
                   <div><input class="topButton" type="button" onclick="window.location = '/Account/VentureLogout';" value="Logout" /></div>
                   <div><input class="topButton" type="button" value="Help" /></div>
                </div>

                <div class="textSizeChangers">
                    <a href="javascript:chooseStyle('none', 60)" style="margin-left: 50px; padding-right:0px;">
                        <img src="/Content/images/font-small.gif" alt="Small font" style="border-width: 0px" />
                    </a>
                    <a href="javascript:chooseStyle('medium', 60)" style="padding-left:0px; padding-right:0px;">
                        <img src="/Content/images/font-medium.gif" alt="Medium font" style="border-width: 0px" />
                    </a>
                    <a href="javascript:chooseStyle('large', 60)" style="padding-left:0px;">
                        <img src="/Content/images/font-large.gif" alt="Large font" style="border-width: 0px" />
                    </a>
                </div>
                <div class="userBox">
                    Current User:
                    <span style="text-decoration:underline;">@Html.Raw(Assmnts.Infrastructure.SessionHelper.LoginInfo.LoginID)</span>
                </div>

                <div class="minutesBox">
                    Session Minutes Remaining:
                    <input id="minutesRemaining" style="color:red; width:50px;" type="text" disabled value="" />
                </div>
            </div>
        </nav>
    </header>
    <div>
        <img class="sisImage" src="~/Content/images/SISOnline logo.jpg" />

        <div class="topBoxes">
            <div class="adminBox">
                <div style="float:left;">
                    <div><span class="head">Venture Options</span></div>

                    <div><a href="/Search">Assessments</a></div>
                    <div><a href="/DataSync/Index">View Assessment Download</a></div>
                    <div><a href="/DataSync/MetaSync">Download Form Data</a></div>
                    <div><a href="/DataSync/Upload">Upload Completed Assessments</a></div>
                </div>
            </div>
        </div>
    </div>

    <div id="main" class="col-sm-10">
        @RenderBody()

        <div id="returnToAssessments">
            <input type="button" value="Return To Assessments" onclick="returnResults()" />
        </div>
    </div>
</body>
</html>