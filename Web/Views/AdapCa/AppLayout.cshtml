@model Assmnts.Models.TemplateItems
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->

    <meta name="author" content="">
    <meta name="description" content="">
    <!--<link rel="shortcut icon" href="../../favicon.ico">-->

    <title>Demographics Information</title>

    <!--jQuery -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>

    <!-- bootstrap -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>

    @Scripts.Render("~/bundles/CAJS")
    @Styles.Render("~/Content/CAAdap")
    <script>
        $(document).ready(function () {
            $('.input-group.date').datepicker({
                autoclose: true,
                format: 'mm/dd/yyyy'
            });
        });
    </script>
    <script>
        $(document).ready(function () {
            //setup certain elements to be visible iff a certain checkbox is checked
            $('#AsianAncestry').toggle($('#ADAP_D3_Asian').get(0).checked);
            $('#ADAP_D3_Asian').change(function () { $('#AsianAncestry').toggle($(this).get(0).checked); });

            $('#PIAncestry').toggle($('#ADAP_D3_NativeHPI').get(0).checked);
            $('#ADAP_D3_NativeHPI').change(function () { $('#PIAncestry').toggle($(this).get(0).checked); });
        });
    </script>

</head>
<body>
    <!-- Fixed navbar -->
    <nav id="custom-bootstrap-menu" class="navbar navbar-default navbar-static-top" style="margin:0; border-bottom: 1px solid #d5d5d5">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="home.html" class="logo pull-left"><img src="~/Content/images/ca_small.png" class="img-responsive" alt="CDPH" /></a>
            </div>
            <div id="navbar" class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="dropdown active">
                        <a href="" class="dropdown-toggle" data-toggle="dropdown"><i class="glyphicon glyphicon-copy"></i> Placeholder<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li><a href="">Placeholder</a></li>
                            <li class="divider"></li>
                            <li><a href="">Placeholder</a></li>
                            <li class="divider"></li>
                            <li><a href="">Placeholder</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href=""><i class="glyphicon glyphicon-book"></i> Placeholder</a>
                    </li>
                    <li>
                        <a href=""><i class="glyphicon glyphicon-credit-card"></i> Placeholder</a>
                    </li>
                    <li>
                        <a href=""><i class="glyphicon glyphicon-comment"></i> Placeholder</a>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="glyphicon glyphicon-user"></i> Bill Maas<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li><a href="">Profile Information</a></li>
                            <li class="divider"></li>
                            <li><a href="">Logout</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="" style="color:#eaa002"><i class="glyphicon glyphicon-cog"></i> Administration</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="container" style="padding-top:0">
        <div id="ADAPHeader">
            <div id="ADAPHeaderText"><h3>Demographics Information</h3> </div>
            <hr class="hr-dark">
        </div>
    </div>
    <div class="container" style="padding-bottom:75px; padding-top:15px;">
        <form class="form-horizontal" id="adapForm" action="/Results/Save" method="post" enctype="multipart/form-data">
            <input type="hidden" id="navPartId" name="navPartId" value="@Model.thisPartId"/>
            <input type="hidden" id="navSectionId" name="navSectionId" value="@Model.thisSectionId" />
            @RenderBody()
            <input type="submit" value="Save" id="btnSubmit" />
        </form>
    </div>
    <footer class="footer">
        <div class="container">
            <div class="row">
                <div class="col-md-5">
                    <img id="profile-img" class="img-responsive" src="~/Content/images/lifia_logo-sm.png" alt="lifia" />
                </div>
                <div class="col-md-7 top-buffer-5">
                    <p class="text-right">
                        <span class="text-muted small" style="color:#d5d5d5">Copyright &copy; 2012-2016 A.J. Boggs & Co. <br /> All rights reserved.</span>
                    </p>
                </div>
            </div>
        </div>
    </footer>
</body>
</html>